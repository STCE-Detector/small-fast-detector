{
  "name": "qualitative",

  "tracker_name": "ByteTrack",

  "source_weights_path": "./detectors/8sp2_150e_64b.engine",
  "source_stream_path": "./videos/output.mp4",

  "device": "cuda",
  "fps_reduction": 0,

  "save_dir": "./outputs/",
  "save_results": false,
  "save_video": false,
  "display": true,
  "sync_fps": false,
  "stabilize": false,
  "logging": false,
  "stream_mode": false,
  "compression_mode": true,

  "img_size": 640,
  "conf_threshold": 0.1,
  "iou_threshold": 0.7,
  "max_det": 300,
  "agnostic_nms": false,

  "tracker_args": {
    "track_high_thresh": 0.4,
    "track_low_thresh": 0.1,
    "new_track_thresh": 0.25,
    "track_buffer": 30,
    "match_thresh": 0.9,
    "proximity_thresh": 0.9,
    "appearance_thresh": 0.8,
    "with_reid": false,
    "reid_default": false,
    "weight_path": "ver12.pt",
    "gmc_method": null,
    "device": "cuda"
  },

  "action_recognition": {
    "enabled": true,
    "speed_projection": [0.7, 1.5],
    "_comment": "",

    "gather": {
      "enabled": true,
      "min_people": 2,
      "distance_threshold": 1.0,
      "area_threshold": 0.5,
      "_comment": ""
    },

    "stand_still": {
      "enabled": true,
      "speed_threshold": 0.001,
      "_comment": ""
    },

    "fast_approach": {
      "enabled": true,
      "draw": true,
      "distance_threshold": 4,
      "speed_threshold": 0.005,
      "_comment": ""
    },

    "suddenly_run": {
      "enabled": true,
      "speed_threshold": 0.025,
      "_comment": ""
    },

    "overstep_boundary": {
      "enabled": true,
      "draw": true,
      "line": [0, 0, 1920, 1080],
      "region": "top"
    }
  }
}

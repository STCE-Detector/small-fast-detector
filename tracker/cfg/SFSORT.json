{
  "name": "qualitative",

  "tracker_name": "SFSORT",

  "source_weights_path": "./detectors/8sp2_150.pt",
  "source_stream_path": "/Users/inaki-eab/Desktop/TAR_data/selection/11.mp4",

  "device": "mps",
  "fps_reduction": 0,

  "save_dir": "./outputs/",
  "save_results": false,
  "save_video": false,
  "display": true,
  "stabilize": false,
  "logging": false,
  "stream_mode": false,
  "compression_mode": true,

  "img_size": 640,
  "conf_threshold": 0.1,
  "iou_threshold": 0.7,
  "max_det": 50,
  "agnostic_nms": true,

  "tracker_args": {
    "dynamic_tuning": false,
    "cth": 0.7,
    "high_th": 0.55,
    "high_th_m": 0.07,
    "match_th_first": 0.55,
    "match_th_first_m": 0.05,
    "match_th_second": 0.5,
    "low_th": 0.1,
    "new_track_th": 0.55,
    "new_track_th_m": 0.2
  },

  "action_recognition": {
    "enabled": false,
    "speed_projection": [0.7, 1.5],
    "_comment": "",

    "gather": {
      "enabled": true,
      "min_people": 2,
      "distance_threshold": 1.0,
      "area_threshold": 0.5,
      "_comment": ""
    },

    "stand_still": {
      "enabled": true,
      "speed_threshold": 0.001,
      "_comment": ""
    },

    "fast_approach": {
      "enabled": true,
      "draw": true,
      "distance_threshold": 4,
      "speed_threshold": 0.005,
      "_comment": ""
    },

    "suddenly_run": {
      "enabled": true,
      "speed_threshold": 0.025,
      "_comment": ""
    },

    "overstep_boundary": {
      "enabled": true,
      "draw": true,
      "line": [0, 0, 1920, 1080],
      "region": "top"
